{
  "extends": ["airbnb-base", "prettier", "plugin:security/recommended"],
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "global-require": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "camelcase": 0,
    "no-unused-expressions": 0
  },
  "plugins": ["import", "security"]
}
